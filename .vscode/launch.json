	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch demo1",
			"program": "${workspaceRoot}\\examples\\demo1\\server.js",
			"cwd": "${workspaceRoot}\\examples\\demo1"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch demo2",
			"program": "${workspaceRoot}\\examples\\demo2\\index.js",
			"cwd": "${workspaceRoot}\\examples\\demo2"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch benchmark",
			"program": "${workspaceRoot}\\benchmark\\index.js",
			"args": ["transports"],
			"cwd": "${workspaceRoot}\\benchmark"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest",
			"program": "${workspaceRoot}\\node_modules\\jest-cli\\bin\\jest.js",
			"args": ["--runInBand"],
			"cwd": "${workspaceRoot}",
			"runtimeArgs": [
                "--nolazy"
            ]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Process",
			"port": 5858
		}
	]
}