{
  "name": "moleculer",
  "version": "0.4.0",
  "description": "Fast & powerful microservices framework for Node.JS",
  "main": "index.js",
  "scripts": {
    "demo": "node examples/index.js",
    "demow": "nodemon examples/index.js",
    "bench": "node benchmark/index.js",
    "perf": "nodemon --allow-natives-syntax perf-runner.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "update-deps": "next-update -k false --tldr",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservices",
    "moleculer",
    "service",
    "services",
    "micro",
    "microservice",
    "micro-services",
    "micro services",
    "worker",
    "workers",
    "microworker",
    "microworkers",
    "rpc",
    "pubsub",
    "scalable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-services/moleculer.git"
  },
  "author": "Ice Services",
  "license": "MIT",
  "devDependencies": {
    "benchmarkify": "1.0.0",
    "coveralls": "2.11.16",
    "eslint": "3.15.0",
    "express": "4.14.1",
    "fakerator": "0.3.0",
    "jest": "18.1.0",
    "lolex": "1.5.2",
    "next-update": "1.5.1",
    "socket.io": "1.7.3"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "testPathDirs": [
      "../test"
    ]
  },
  "dependencies": {
    "amqplib": "0.5.1",
    "bluebird": "3.4.7",
    "chalk": "1.1.3",
    "es6-error": "4.0.2",
    "eventemitter2": "3.0.0",
    "glob": "7.1.1",
    "ioredis": "2.5.0",
    "lodash": "4.17.4",
    "micromatch": "2.3.11",
    "mqtt": "2.4.0",
    "nats": "0.7.6",
    "node-object-hash": "1.1.6",
    "v8-natives": "0.0.3",
    "validatorjs": "3.11.0"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "tonicExampleFilename": "docs/runkit/simple.js",
  "typings": "./index.d.ts"
}
