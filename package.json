{
  "name": "ice-services",
  "version": "0.2.0",
  "description": "Microservices framework for Node.JS",
  "main": "index.js",
  "scripts": {
    "demo1": "nodemon examples/demo1/server.js",
    "demo2": "nodemon examples/demo2/index.js",
    "bench": "node benchmark/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "service",
    "services",
    "microservice",
    "microservices",
    "micro-services",
    "micro services",
    "worker",
    "workers",
    "microworker",
    "microworkers",
    "pubsub"
  ],
  "author": "Icebob",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "2.1.3",
    "coveralls": "2.11.15",
    "eslint": "3.13.0",
    "fakerator": "0.3.0",
    "fast-json-parse": "1.0.2",
    "fast-safe-stringify": "1.1.3",
    "jest": "18.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "testPathDirs": [
      "../test"
    ]
  },
  "dependencies": {
    "chalk": "1.1.3",
    "eventemitter2": "2.2.1",
    "glob": "7.1.1",
    "ioredis": "2.5.0",
    "lodash": "4.17.4",
    "minimatch": "3.0.3",
    "nats": "0.6.8",
    "object-hash": "1.1.5",
    "uuid": "3.0.1",
    "uuid-token-generator": "0.5.0"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "typings": "./index.d.ts"
}
